{
  "name": "nuwber/rabbitevents",
  "description": "Laravel back-to-back broadcasting events. It uses RabbitMQ as the transport.",
  "keywords": [
    "queue",
    "laravel",
    "rabbitmq",
    "events",
    "event bus",
    "bus"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Kirdzei",
      "email": "masterjus@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-bcmath": "*",
    "ext-pcntl": "*",
    "ext-json": "*",
    "illuminate/events": "^7.0 | ^8.0 | ^9.0",
    "illuminate/queue": "^7.0 | ^8.0 | ^9.0",
    "illuminate/support": "^7.0 | ^8.0 | ^9.0",
    "enqueue/amqp-lib": "^0.10"
  },
  "require-dev": {
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Nuwber\\Events\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nuwber\\Events\\RabbitEventsServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nuwber\\Events\\Tests\\": "tests/"
    }
  }
}
